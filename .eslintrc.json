{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "plugins": ["prettier", "import"],
    "rules": {
        "prettier/prettier": "error",
        "camelcase": "error",
        "object-curly-spacing": [
            2,
            "always"
        ],
        "no-multiple-empty-lines": [
            "error",
            {
                "max": 2,
                "maxEOF": 1
            }
        ],
        "object-shorthand": [
            "error"
        ]
    }
}
